{
    "routes": [
      {
        "route": "/",
        "rewrite": "/index.html"
      },
      {
        "route": "/aboutUs",
        "rewrite": "/index.html"
      },
      {
        "route": "/profile",
        "rewrite": "/index.html"
      },
      {
        "route": "/video",
        "rewrite": "/index.html"
      },
      {
        "route": "/*",
        "rewrite": "/index.html"
      }
    ],
    "navigationFallback": {
      "rewrite": "/index.html",
      "exclude": ["/images/*.{png,jpg,gif}", "/css/*"]
    },
    "responseOverrides": {
      "404": {
        "rewrite": "/index.html"
      }
    }
  }
  